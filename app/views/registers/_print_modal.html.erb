<div class="modal fade" id="registersPrintModal" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="newCollaboratorModalLabel">Seleccione rango de Fechas</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">Ã—</span>
        </button>
      </div>
      <%= form_for(:q, url: print_registers_path, method: :get) do |f|%>
        <div class="modal-body">
          <div class="form-group">
            <div class="row mb-1">
              <div class="col-md-6 mb-1">
                <%= f.text_field :event_date_between, class: "form-control daterange", placeholder: "Fecha Inicio", required: true, autocomplete: "off"%>
              </div>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="submit" class="btn btn-primary">Continuar</button>
        </div>
      <%end%>
    </div>
  </div>
</div>
